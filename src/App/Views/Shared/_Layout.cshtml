<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/static/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/static/bootstrap/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/static/styles.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/static/js/jquery-1.7.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/bootstrap/js/bootstrap-dropdown.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/bootstrap/js/bootstrap-collapse.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/js/knockout-2.1.0rc2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/js/base.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/js/utilities.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/js/App.ErrorManager.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/js/DateUtils.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/js/json2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/static/js/moment.min.js")" type="text/javascript"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body>
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"><span
                    class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                </a><a class="brand" href="#">AppName here</a>
                <div class="nav-collapse">
                    <ul class="nav">
                        <li class="active">@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        @Html.Partial("_LogOnPartial")
                    </ul>
                </div>
                <!--/.nav-collapse -->
            </div>
        </div>
    </div>
    <div class="container">
        @RenderBody()
    </div>
    
    <footer>
        
      </footer>
</body>
</html>
